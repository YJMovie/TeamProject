<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="member">
	<!-- resultType = "DB의 레코드와 자료저장 클래스 mapping" -->
	<!-- <select id="list" resultType="join.joinDTO">
		select * from join order by desc
	</select> -->
	
	<insert id="insert">
		insert into member_info values (
		  #{userid},#{userpwd},
		  #{name},#{gender},#{phone},#{address},
		  #{email},sysdate,2
		)
	</insert>
	<select id="confirmID" parameterType="String" resultType="String">
		select userid from member_info where userid = #{userid}
	</select>
</mapper>